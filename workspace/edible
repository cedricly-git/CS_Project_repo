        "luffa-aegyptiaca", "mangifera-indica", "manihot-esculenta", "medicago-sativa", "moringa-oleifera", 
        "ocimum-tenuiflorum", "opuntia-stricta", "oryza-sativa", "pastinaca-sativa", "perilla-frutescens",
        "persea-americana", "petroselinum-crispum", "physalis-peruviana", "pistacia-vera", "plukenetia-volubilis", 
        "saccharum-officinarum", "salvia-hispanica", "sesamum-indicum"
    
])

new_flower_slugs = []
page = 1
while len(new_flower_slugs) < 100:
    response = requests.get(BASE_URL, params={
        "token": API_KEY,
        "page": page,
        "filter[family]": "Asteraceae",  # or Rosaceae, etc.
    })

    if response.status_code != 200:
        print(f"Error: {response.status_code}")
        break

    data = response.json().get("data", [])
    if not data:
        break

    for plant in data:
        slug = plant["slug"]
        if slug not in existing_flower_slugs and slug not in new_flower_slugs:
            new_flower_slugs.append(slug)
            if len(new_flower_slugs) == 100:
                break

    page += 1

# Output result
print("NEW FLOWER SPECIES:")
for slug in new_flower_slugs:
    print(f'"{slug}",')